{data.cDesc}}</nutcat-text>\n      </template>\n    </div>\n    <slot></slot>\n  </div>\n</template>\n\n<script lang=\"ts\">\nimport { createComponent } from '@/utils/create';\nconst { componentName, create } = createComponent('shop');\n\nexport default create({\n  props: {\n    data: {\n      type: [Array, Object],\n      default: function() {\n        return [];\n      }\n    },\n    col: {\n      type: [String, Number],\n      default: '1'\n    },\n    type: {\n      type: String,\n      default: ''\n    },\n    nameRow: {\n      type: [String, Number],\n      default: '1'\n    },\n    descRow: {\n      type: [String, Number],\n      default: '1'\n    },\n    tag: {\n      type: String,\n      default: ''\n    },\n    bgColor: {\n      type: String,\n      default: ''\n    },\n    bgUrl: {\n      type: String,\n      default: ''\n    },\n    imgWidth: {\n      type: [String, Number],\n      default: ''\n    },\n    imgHeight: {\n      type: [String, Number],\n      default: ''\n    }\n  },\n  data() {\n    return {};\n  },\n  mounted() {},\n  computed: {\n    classes() {\n      return {\n        [componentName]: true,\n        [`${componentName}-${(this as any).col}`]: (this as any).col,\n        [`${componentName}-${(this as any).type}`]: (this as any).type,\n        [`${componentName}-scrollX`]: (this as any).scrollX\n      };\n    },\n    styles() {\n      if ((this as any).bgUrl) {\n        return {\n          'background-image': `url(${(this as any).bgUrl})`,\n          'background-repeat': 'no-repeat',\n          'background-position': 'center',\n          'background-size': '100% 100%'\n        };\n      }\n      if ((this as any).bgColor) {\n        return {\n          'background-color': `${(this as any).bgColor}`\n        };\n      }\n    }\n  },\n  methods: {\n    toShop(item) {\n      (this as any).$emit('click-shop', item);\n    }\n  }\n});\n</script>\n\n<style lang=\"scss\" scoped>\n@import 'index.scss';\n$prefix: nutcat-shop;\n\n.#{$prefix}-normal-3 {\n  display: flex;\n  width: 100%;\n  padding: 12px 10px;\n  background: #fff;\n  .logo-box {\n    width: 56px;\n    height: 28px;\n    margin-top: 3px;\n    margin-right: 10px;\n  }\n  .shop-content {\n    width: calc(100% - 58px - 10px);\n    text-align: left;\n  }\n  .sub-name {\n    margin-bottom: 2px;\n    line-height: 1.2;\n    // font-size: 12px;\n  }\n  .sub-desc {\n    line-height: 1.2;\n    // font-size: 14px;\n  }\n  &.nutcat-shop-2 {\n    width: 178px;\n  }\n}\n</style>\n"]}]}