e.slice(1);\n    // if (!isMobile && this.$route.hash != hash) {\n    //   this.title = this.$route.name;\n    // } else {\n    //   this.title = this.$route.name;\n    // }\n  },\n  methods: {\n    //返回demo页\n    goBack() {\n      this.$router.back();\n    }\n  }\n  \n}\n</script>\n\n<style lang=\"scss\">\n@import '../../styles/font/iconfont.css';\n#app {\n  font-family: PingFangSC-Regular;\n  -webkit-font-smoothing: antialiased;\n  -moz-osx-font-smoothing: grayscale;\n  -webkit-text-size-adjust: none;\n  -ms-text-size-adjust: 100%;\n  background: #fff;\n  // height: 100%;\n  width: 100%;\n  display: flex;\n  flex-direction: column;\n\n  #nav {\n    position: fixed;\n    z-index: 10;\n    left: 0;\n    right: 0;\n    height: 57px;\n    line-height: 57px;\n    text-align: center;\n    background: $white;\n    font-weight: bold;\n    font-size: 20px;\n    color: rgba(51, 51, 51, 1);\n    box-shadow: 0px 4px 10px 0px rgba(0, 0, 0, 0.07);\n    .back {\n      position: absolute;\n      left: 0;\n      height: 100%;\n      width: 50px;\n      display: flex;\n      align-items: center;\n      justify-content: center;\n      cursor: pointer;\n    }\n  }\n\n  .demo {\n    height: 100%;\n    background: #f7f8fa;\n    overflow-x: hidden;\n    overflow-y: auto;\n    padding: 57px 9px 0 9px;\n    // padding-left: 9px;\n    // padding-right: 9px;\n    &.full {\n      padding: 57px 0 0 0;\n      h2 {\n        padding-left: 25px;\n      }\n    }\n    &.cell {\n      padding-left: 18px;\n      padding-right: 18px;\n    }\n\n    &.bg-w {\n      background: #fff;\n    }\n\n    &::-webkit-scrollbar {\n      width: 0;\n      background: transparent;\n    }\n    h2 {\n      // height: 56px;\n      margin-top: 30px;\n      margin-bottom: 15px;\n      line-height: 23px;\n      font-size: 16px;\n      color: $title-color2;\n      font-weight: 500;\n      + .type {\n        margin-top: -5px;\n      }\n    }\n    h3 {\n      padding: 20px 16px 16px;\n      line-height: 23px;\n      font-size: 14px;\n      color: $title-color2;\n      font-weight: 500;\n    }\n    .type {\n      // margin-top: -5px;\n      margin-top: 15px;\n      margin-bottom: 15px;\n      line-height: 20px;\n      color: #909CA4;\n      font-size: 14px;\n    }\n    .card {\n      padding: 25px 18px;\n      background: rgba(255, 255, 255, 1);\n    }\n  }\n}\n.nut-icon {\n  width: $icon-width;\n  height: $icon-height;\n  line-height: $icon-line-height;\n  text-align: right;\n\n  &__img {\n    width: $icon-width;\n    height: $icon-height;\n    object-fit: contain;\n  }\n}\n</style>\n"]}]}