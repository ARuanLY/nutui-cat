d='10' :standTime='1000' :showIcon='true' color='#14100D' background='transparent'>\n        <template v-slot:rightIcon>\n          <nutcat-icon style=\"font-size: 20px;\" color=\"#F02B2B\" type=\"qiang\"></nutcat-icon>\n        </template>\n      </nut-noticebar>\n    </div>\n  </div>\n</template>\n  \n<script>\nimport { createComponent } from '@/utils/create';\nconst { createDemo } = createComponent('noticebar');\nexport default createDemo({\n  props: {},\n  data() {\n    return {\n      horseLamp1: ['惊喜红包免费领', '爆款准点秒', '买超值优惠', '赢百万京豆'],\n      horseLamp2: ['惊喜红包免费领', '爆款准点秒', '买超值优惠', '赢百万京豆'],\n      horseLamp3: ['惊喜红包免费领', '爆款准点秒', '买超值优惠', '赢百万京豆'],\n    };\n  },\n\n  mounted() {},\n\n  methods:{\n    go(item){\n      console.log(item)\n    }\n  }\n});\n</script>\n  \n<style lang=\"scss\">\n#app {\n  .demo {\n  }\n}\n.interstroll-list{\n  padding: 0 10px;\n  color:#14100D ;\n  background: linear-gradient(180deg, rgba(255,203,132,1) 0%,rgba(255,230,196,1) 100%);\n  border-radius: 10px;\n  border: 1px solid #eee;\n}\n.custom-item{\n  display: flex;\n  align-items: center;\n  height: 50px;\n  \n}\n</style>\n  "]}]}